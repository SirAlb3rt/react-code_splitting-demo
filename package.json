{
  "name": "react-code_splitting-demo",
  "version": "1.0.0",
  "description": "just a demo with react code-splitting",
  "scripts": {
    "start:dev": "better-npm-run start-development",
    "start:prod": "better-npm-run start-production",
    "clean": "rimraf build"
  },
  "betterScripts": {
    "start-development": {
      "command": "npm run clean && node server/index.js",
      "env": {
        "NODE_ENV": "development",
        "HOST": "localhost",
        "PORT": 3000
      }
    },
    "start-production": {
      "command": "npm run clean && node server/index.js",
      "env": {
        "NODE_ENV": "production",
        "HOST": "0.0.0.0",
        "PORT": 3000
      }
    }
  },
  "author": "https://github.com/SirAlb3rt",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-universal-import": "^1.2.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "chalk": "^2.1.0",
    "ejs": "^2.5.7",
    "eslint-loader": "^1.9.0",
    "express": "^4.15.4",
    "extract-css-chunks-webpack-plugin": "^2.0.16",
    "morgan": "^1.8.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-universal-component": "^2.5.0",
    "redux": "^3.7.2",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.5",
    "webpack-flush-chunks": "^1.1.23",
    "webpack-merge": "^4.1.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.5.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.2.1",
    "prettier": "^1.5.3",
    "prettier-eslint": "^6.4.2",
    "react-hot-loader": "next",
    "redux-logger": "^3.0.6",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-hot-server-middleware": "^0.1.0",
    "write-file-webpack-plugin": "^4.1.0"
  }
